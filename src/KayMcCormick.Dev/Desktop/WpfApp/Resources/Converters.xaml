<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              xmlns:converters="clr-namespace:WpfApp.Core.Converters"
              xmlns:wpf="clr-namespace:KayMcCormick.Lib.Wpf;assembly=KayMcCormick.Lib.Wpf"
              xmlns:xaml="clr-namespace:KayMcCormick.Lib.Wpf.Xaml;assembly=KayMcCormick.Lib.Wpf">
    <!-- Conversion for "InstanceRegistration" -->
    <converters:InstanceRegistrationConverter 
        x:Key="InstanceRegistrationConverter" />
    <converters:AssemblyConverter x:Key="AssemblyConverter" />
    <converters:ComponentLifetimeConverter x:Key="ComponentLifetimeConverter" />
    <converters:RegistrationConverter2 x:Key="RegistrationConverter2"/>
    <converters:RegistrationSourceConverter x:Key="RegistrationSourceConverter"/>
    <converters:ResolveServiceConverter x:Key="MyConverter"/>
    <converters:TargetConverter x:Key="TargetConverter"/>
    <wpf:GetTypeConverter x:Key="GetTypeConverter"/>
    <xaml:XamlXmlDocumentConverter x:Key="XamlXmlDocumentConverter"/>
</ResourceDictionary>
