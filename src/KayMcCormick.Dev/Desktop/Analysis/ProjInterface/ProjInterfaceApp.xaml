<wpf:BaseApp
    xmlns:kayWpfLib="http://kaymccormick.com/xaml/wpflib"
  x:Class="ProjInterface.ProjInterfaceApp"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:anal="http://kaymccormick.com/xaml/analysis"
  xmlns:wpf="clr-namespace:KayMcCormick.Lib.Wpf"
  xmlns:analysisControls="clr-namespace:AnalysisControls"
  xmlns:interfaces="clr-namespace:ProjLib.Interfaces">

    <wpf:BaseApp.Resources>
    <Style TargetType="Menu">
      <Setter Property="FontFamily" Value="Leelawadee"/>
      <Setter Property="FontSize" Value="24.0"/>
    </Style>
    <analysisControls:StatusBarItemContentTemplateSelector x:Key="StatusBarItemContentTemplateSelector" />
    <Style TargetType="StatusBarItem">
    <Setter Property="ContentTemplateSelector" Value="{StaticResource StatusBarItemContentTemplateSelector}"></Setter>
    </Style>

   
  </wpf:BaseApp.Resources>
</wpf:BaseApp>